<%= form_tag do %>
<div class="col-md-6 col-md-offset-3">
  <h1>signup</h1>
  <p>Signup to begin questionbox.</p>
  <h1 class="secret">#thesecret</h1>
  <div class="form-group">
    <%= label_tag :email %>
    <%= text_field_tag :email, "", class: "form-control" %>
  </div>
  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password, "", class: "form-control" %>
  </div>
  <div class="form-group">
    <%= submit_tag "Sign In", class:"btn btn-default" %> or <%= link_to "Sign Up", sign_up_path %>
  </div>
</div>
<% end %>
